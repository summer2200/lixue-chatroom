<body>
    <div class="panel panel-default">
        <div class="panel-heading">
            <!-- <h3 class="panel-title">Panel title</h3> -->
            <span class="glyphicon glyphicon-user"></span>
            <span class="pull-left"><b><i><%= userName %></i></b></span>
            Inbox <span class="badge">42</span>
            <span class="pull-right"><%= date %></span>
        </div>
        <div class="panel-body">
            <div class="tabbable cont">
                <div class="tab-content contents">
                    <div id="pane1" class="tab-pane active">
                        <h4>recent chats</h4>
                        <ul class="list-group">
                            <li class="list-group-item">Name1 history1</li>
                            <li class="list-group-item">Name2 history2</li>
                            <li class="list-group-item">Name3 history3</li>
                            <li class="list-group-item">Name4 history4</li>
                            <li class="list-group-item">Name5 history5</li>
                        </ul>
                    </div>
                    <div id="pane2" class="tab-pane">
                        <h4>your friends</h4>
                        <ul class="list-group">
                            <li class="list-group-item">Friend1 history1</li>
                            <li class="list-group-item">Friend2 history2</li>
                            <li class="list-group-item">Friend3 history3</li>
                            <li class="list-group-item">Friend4 history4</li>
                            <li class="list-group-item">Friend5 history5</li>
                        </ul>
                    </div>
                    <div id="pane3" class="tab-pane">
                        <h4>your groups</h4>
                        <ul class="list-group">
                            <li class="list-group-item">Group1 history1</li>
                            <li class="list-group-item">Group2 history2</li>
                            <li class="list-group-item">Group3 history3</li>
                            <li class="list-group-item">Group4 history4</li>
                            <li class="list-group-item">Group5 history5</li>
                        </ul>
                    </div>
                </div>
                <ul class="nav nav-tabs">
                    <li class="active"><a href="#pane1" data-toggle="tab">Recently</a></li>
                    <li><a href="#pane2" data-toggle="tab">Friends</a></li>
                    <li><a href="#pane3" data-toggle="tab">Groups</a></li>
                </ul>
            </div>
        </div>
    </div>
</body>
</html>
